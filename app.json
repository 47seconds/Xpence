{
  "expo": {
    "name": "Xpence",
    "slug": "47seconds-xpence",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/xpence.png",
    "scheme": "xpence",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/xpence.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.x47seconds.xpence",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/xpence.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/images/xpence.png"
      }
    },
    "owner": "47seconds"
  }
}
